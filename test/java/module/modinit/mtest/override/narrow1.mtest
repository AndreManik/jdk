    static int called;

    public Map<String,VersionConstraint> narrow(ModuleDefinition importer, 
	    Map<String,VersionConstraint> constraints) {
	called++;
	System.out.println("Constraints for " + importer.getName() + ": " + constraints);
	if (importer.getName().equals("m1")) {
	    Map<String,VersionConstraint> newConstraints = new HashMap<String,VersionConstraint>(constraints);
	    newConstraints.put("m2", VersionConstraint.valueOf("[1,1.3)"));
	    System.out.println("New constraints for " + importer.getName() + ": " + newConstraints);
	    return newConstraints;
	} else {
	    return constraints;
	}
    }
